{
  "name": "dbml-converter-extensions",
  "version": "1.0.0",
  "description": "Extended DBML conversion utilities - CSV and Excel output with enhanced formatting",
  "main": "src/index.js",
  "bin": {
    "dbml-convert": "./bin/dbml-convert.js"
  },
  "files": [
    "src/",
    "bin/",
    "pkg/",
    "README.md",
    "LICENSE",
    "conversion_guide.md"
  ],
  "scripts": {
    "convert": "node bin/dbml-convert.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ bin/ test/ pkg/",
    "lint:fix": "eslint src/ bin/ test/ pkg/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "dbml",
    "dbml-cli",
    "excel",
    "xlsx",
    "csv",
    "database",
    "converter",
    "cli",
    "database-modeling",
    "schema",
    "export",
    "extensions",
    "postgres",
    "mysql",
    "sqlite"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/instantraaamen/dbml.git"
  },
  "homepage": "https://github.com/instantraaamen/dbml#readme",
  "bugs": {
    "url": "https://github.com/instantraaamen/dbml/issues"
  },
  "dependencies": {
    "@dbml/core": "^3.6.2",
    "commander": "^14.0.0",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 120000,
    "maxWorkers": 1,
    "maxConcurrency": 1,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/cli/index.js",
      "pkg/**/*.js",
      "!pkg/cli.js"
    ]
  }
}
