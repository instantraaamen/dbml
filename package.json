    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "dbml",
    "excel",
    "csv",
    "database",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dbml/core": "^3.6.2"
  },
  "devDependencies": {
    "jest": "^29.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "pkg/**/*.js",
      "!pkg/cli.js"
    ]
  }
